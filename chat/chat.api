syntax = "v1"

@server(
	jwt: Auth
	group: chat
	middleware : Authority
	prefix: /chat
)
service chat {
	//    @handler SendMsg
	//    get /send
	@handler Chat
	get /chat
	@handler SendMsg
	post /sub (SendMsgReq)
	//	@handler Pub
	//	post /pub
}

type (
	SendMsgReq {
		Msg string `json:"msg"`
		Uid string `json:"uid"`
	}
	SendMsgResp {
	}
)